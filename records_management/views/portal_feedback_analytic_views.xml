<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- List view for portal.feedback.analytic -->
    <record id="portal_feedback_analytic_view_tree" model="ir.ui.view">
        <field name="name">portal.feedback.analytic.view.tree</field>
        <field name="model">portal.feedback.analytic</field>
        <field name="arch" type="xml">
        <list>
        <field name="name"/>
        </list>
    </field>
    </record>

<!-- Form view for portal.feedback.analytic -->
    <record id="portal_feedback_analytic_view_form" model="ir.ui.view">
        <field name="name">portal.feedback.analytic.view.form</field>
        <field name="model">portal.feedback.analytic</field>
        <field name="arch" type="xml">
        <form string="Portal.Feedback.Analytic">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
    </record>

<!-- Search view for portal.feedback.analytic -->
    <record id="portal_feedback_analytic_view_search" model="ir.ui.view">
        <field name="name">portal.feedback.analytic.view.search</field>
        <field name="model">portal.feedback.analytic</field>
        <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
    </record>

<!-- Action for portal.feedback.analytic -->
    <record id="action_portal_feedback_analytic" model="ir.actions.act_window">
        <field name="name">Portal.Feedback.Analytic</field>
        <field name="res_model">portal.feedback.analytic</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            Click to add a new portal.feedback.analytic.
        </p>
    </field>
    </record>

</odoo>
