<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- List view for base.rate -->
    <record id="shredding_service_log_view_tree" model="ir.ui.view">
        <field name="name">shredding.service.log.view.tree</field>
        <field name="model">shredding.service.log</field>
        <field name="arch" type="xml">
        <tree>
        <field name="name"/>
        </tree>
    </field>
    </record>

<!-- Form view for base.rate -->
    <record id="shredding_service_log_view_form" model="ir.ui.view">
        <field name="name">shredding.service.log.view.form</field>
        <field name="model">shredding.service.log</field>
        <field name="arch" type="xml">
        <form string="Shredding Service Log">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
    </record>

<!-- Search view for base.rate -->
    <record id="shredding_service_log_view_search" model="ir.ui.view">
        <field name="name">shredding.service.log.view.search</field>
        <field name="model">shredding.service.log</field>
        <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
    </record>

<!-- List view for Action -->
    <record id="action_shredding_service_log" model="ir.actions.act_window">
        <field name="name">Shredding Service Log</field>
        <field name="res_model">shredding.service.log</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            There is no examples click here to add new Shredding Service Log.
        </p>
    </field>
    </record>

</odoo>
