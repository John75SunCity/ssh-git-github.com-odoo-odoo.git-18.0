<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- List view for base.rate -->
    <record id="survey_user_input_enhanced_view_tree" model="ir.ui.view">
        <field name="name">survey.user.input.enhanced.view.tree</field>
        <field name="model">survey.user.input.enhanced</field>
        <field name="arch" type="xml">
        <tree>
        <field name="name"/>
        </tree>
    </field>
    </record>

<!-- Form view for base.rate -->
    <record id="survey_user_input_enhanced_view_form" model="ir.ui.view">
        <field name="name">survey.user.input.enhanced.view.form</field>
        <field name="model">survey.user.input.enhanced</field>
        <field name="arch" type="xml">
        <form string="Survey User Input Enhanced">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
    </record>

<!-- Search view for base.rate -->
    <record id="survey_user_input_enhanced_view_search" model="ir.ui.view">
        <field name="name">survey.user.input.enhanced.view.search</field>
        <field name="model">survey.user.input.enhanced</field>
        <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
    </record>

<!-- List view for Action -->
    <record id="action_survey_user_input_enhanced" model="ir.actions.act_window">
        <field name="name">Survey User Input Enhanced</field>
        <field name="res_model">survey.user.input.enhanced</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            There is no examples click here to add new Survey User Input Enhanced.
        </p>
    </field>
    </record>

</odoo>
