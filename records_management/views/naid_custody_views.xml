<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
<!-- View naid.custody View List -->
    <record id="view_naid_custody_list" model="ir.ui.view">
        <field name="name">view.naid.custody.list</field>
        <field name="model">naid.custody</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View naid.custody form -->
    <record id="view_naid_custody_form" model="ir.ui.view">
        <field name="name">view.naid.custody.form</field>
        <field name="model">naid.custody</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <form string="Naid Custody">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View naid.custody search -->
    <record id="view_naid_custody_search" model="ir.ui.view">
        <field name="name">view.naid.custody.search</field>
        <field name="model">naid.custody</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action naid.custody -->
    <record id="action_naid_custody" model="ir.actions.act_window">
        <field name="name">Naid Custody</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">naid.custody</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Naid Custody.
            </p>
        </field>
    </record>
</data>

</odoo>
