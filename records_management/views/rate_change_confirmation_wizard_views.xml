<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View rate.change.confirmation.wizard View List -->
    <record id="view_rate_change_confirmation_wizard_list" model="ir.ui.view">
        <field name="name">view.rate.change.confirmation.wizard.list</field>
        <field name="model">rate.change.confirmation.wizard</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View rate.change.confirmation.wizard form -->
    <record id="view_rate_change_confirmation_wizard_form" model="ir.ui.view">
        <field name="name">view.rate.change.confirmation.wizard.form</field>
        <field name="model">rate.change.confirmation.wizard</field>
        <field name="arch" type="xml">
            <form string="Rate Change Confirmation Wizard">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View rate.change.confirmation.wizard search -->
    <record id="view_rate_change_confirmation_wizard_search" model="ir.ui.view">
        <field name="name">view.rate.change.confirmation.wizard.search</field>
        <field name="model">rate.change.confirmation.wizard</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action rate.change.confirmation.wizard -->
    <record id="action_rate_change_confirmation_wizard" model="ir.actions.act_window">
        <field name="name">Rate Change Confirmation Wizard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">rate.change.confirmation.wizard</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Rate Change Confirmation Wizard.
            </p>
        </field>
    </record>

</odoo>
