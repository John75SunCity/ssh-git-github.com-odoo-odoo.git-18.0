<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
<!-- View file_retrieveal_work_order_item.py View List -->
    <record id="view_file_retrieveal_work_order_item_py_list" model="ir.ui.view">
        <field name="name">view.file_retrieveal_work_order_item.py.list</field>
        <field name="model">file_retrieveal_work_order_item.py</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View file_retrieveal_work_order_item.py form -->
    <record id="view_file_retrieveal_work_order_item_py_form" model="ir.ui.view">
        <field name="name">view.file_retrieveal_work_order_item.py.form</field>
        <field name="model">file_retrieveal_work_order_item.py</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="File_retrieveal_work_order_item Py">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View file_retrieveal_work_order_item.py search -->
    <record id="view_file_retrieveal_work_order_item_py_search" model="ir.ui.view">
        <field name="name">view.file_retrieveal_work_order_item.py.search</field>
        <field name="model">file_retrieveal_work_order_item.py</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action file_retrieveal_work_order_item.py -->
    <record id="action_file_retrieveal_work_order_item_py" model="ir.actions.act_window">
        <field name="name">File_retrieveal_work_order_item Py</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">file_retrieveal_work_order_item.py</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new File_retrieveal_work_order_item Py.
            </p>
        </field>
    </record>
</data>

</odoo>
