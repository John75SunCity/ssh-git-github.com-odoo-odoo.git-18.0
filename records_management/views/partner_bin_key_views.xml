<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
<!-- Tree view for partner.bin.key -->
    <record id="partner_bin_key_view_tree" model="ir.ui.view">
        <field name="name">partner.bin.key.view.tree</field>
        <field name="model">partner.bin.key</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for partner.bin.key -->
    <record id="partner_bin_key_view_form" model="ir.ui.view">
        <field name="name">partner.bin.key.view.form</field>
        <field name="model">partner.bin.key</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Partner.Bin.Key">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for partner.bin.key -->
    <record id="partner_bin_key_view_search" model="ir.ui.view">
        <field name="name">partner.bin.key.view.search</field>
        <field name="model">partner.bin.key</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action for partner.bin.key -->
    <record id="action_partner_bin_key" model="ir.actions.act_window">
        <field name="name">Partner.Bin.Key</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">partner.bin.key</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to add a new partner.bin.key.
            </p>
        </field>
    </record>
</data>

</odoo>
