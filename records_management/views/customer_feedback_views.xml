<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Tree View -->
        <record id="customer_feedback_view_tree" model="ir.ui.view">
            <field name="name">customer.feedback.view.tree</field>
            <field name="model">customer.feedback</field>
            <field name="type">tree</field>
        <field name="arch" type="xml">
                <tree string="Customer Feedback">
                    <field name="name"/>
                    <field name="company_id"/>
                    <field name="user_id"/>
                    <field name="active"/>
                    <field name="state"/>
                    <field name="priority"/>
                </tree>
            </field>
        </record>

        <!-- Form View -->
        <record id="customer_feedback_view_form" model="ir.ui.view">
            <field name="name">customer.feedback.view.form</field>
            <field name="model">customer.feedback</field>
            <field name="type">form</field>
        <field name="arch" type="xml">
                <form string="Customer Feedback">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="company_id"/>
                            <field name="user_id"/>
                            <field name="active"/>
                            <field name="state"/>
                            <field name="priority"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Search View -->
        <record id="customer_feedback_view_search" model="ir.ui.view">
            <field name="name">customer.feedback.view.search</field>
            <field name="model">customer.feedback</field>
            <field name="type">search</field>
        <field name="arch" type="xml">
                <search string="Search Customer Feedback">
                    <!-- Add search fields here -->
                </search>
            </field>
        </record>

        <!-- Action -->
        <record id="action_customer_feedback" model="ir.actions.act_window">
            <field name="name">Customer Feedback</field>
            <field name="res_model">customer.feedback</field>
            <field name="view_mode">list,form</field>
        </record>

        <!-- Menu -->
        <!--
        <menuitem
            id="menu_customer_feedback"
            name="Customer Feedback"
            action="action_customer_feedback"
            parent="REPLACE_WITH_PARENT_MENU_ID"
            sequence="10"/>
        -->

    </data>
</odoo>
