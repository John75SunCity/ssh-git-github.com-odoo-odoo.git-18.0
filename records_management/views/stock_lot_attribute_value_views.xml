<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View stock.lot.attribute.value View List -->
    <record id="view_stock_lot_attribute_value_list" model="ir.ui.view">
        <field name="name">view.stock.lot.attribute.value.list</field>
        <field name="model">stock.lot.attribute.value</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View stock.lot.attribute.value form -->
    <record id="view_stock_lot_attribute_value_form" model="ir.ui.view">
        <field name="name">view.stock.lot.attribute.value.form</field>
        <field name="model">stock.lot.attribute.value</field>
        <field name="arch" type="xml">
            <form string="Stock Lot Attribute Value">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View stock.lot.attribute.value search -->
    <record id="view_stock_lot_attribute_value_search" model="ir.ui.view">
        <field name="name">view.stock.lot.attribute.value.search</field>
        <field name="model">stock.lot.attribute.value</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action stock.lot.attribute.value -->
    <record id="action_stock_lot_attribute_value" model="ir.actions.act_window">
        <field name="name">Stock Lot Attribute Value</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.lot.attribute.value</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Stock Lot Attribute Value.
            </p>
        </field>
    </record>

</odoo>
