<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View barcode.models.enhanced View List -->
    <record id="view_barcode_models_enhanced_list" model="ir.ui.view">
        <field name="name">view.barcode.models.enhanced.list</field>
        <field name="model">barcode.models.enhanced</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View barcode.models.enhanced form -->
    <record id="view_barcode_models_enhanced_form" model="ir.ui.view">
        <field name="name">view.barcode.models.enhanced.form</field>
        <field name="model">barcode.models.enhanced</field>
        <field name="arch" type="xml">
            <form string="Barcode Models Enhanced">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View barcode.models.enhanced search -->
    <record id="view_barcode_models_enhanced_search" model="ir.ui.view">
        <field name="name">view.barcode.models.enhanced.search</field>
        <field name="model">barcode.models.enhanced</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action barcode.models.enhanced -->
    <record id="action_barcode_models_enhanced" model="ir.actions.act_window">
        <field name="name">Barcode Models Enhanced</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">barcode.models.enhanced</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Barcode Models Enhanced.
            </p>
        </field>
    </record>

</odoo>
