<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View unlock.service.part View List -->
    <record id="view_unlock_service_part_list" model="ir.ui.view">
        <field name="name">view.unlock.service.part.list</field>
        <field name="model">unlock.service.part</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View unlock.service.part form -->
    <record id="view_unlock_service_part_form" model="ir.ui.view">
        <field name="name">view.unlock.service.part.form</field>
        <field name="model">unlock.service.part</field>
        <field name="arch" type="xml">
            <form string="Unlock Service Part">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View unlock.service.part search -->
    <record id="view_unlock_service_part_search" model="ir.ui.view">
        <field name="name">view.unlock.service.part.search</field>
        <field name="model">unlock.service.part</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action unlock.service.part -->
    <record id="action_unlock_service_part" model="ir.actions.act_window">
        <field name="name">Unlock Service Part</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">unlock.service.part</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Unlock Service Part.
            </p>
        </field>
    </record>

</odoo>
