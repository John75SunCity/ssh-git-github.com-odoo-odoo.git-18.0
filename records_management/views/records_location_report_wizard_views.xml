<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View records.location.report.wizard View List -->
    <record id="view_records_location_report_wizard_list" model="ir.ui.view">
        <field name="name">view.records.location.report.wizard.list</field>
        <field name="model">records.location.report.wizard</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View records.location.report.wizard form -->
    <record id="view_records_location_report_wizard_form" model="ir.ui.view">
        <field name="name">view.records.location.report.wizard.form</field>
        <field name="model">records.location.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Records Location Report Wizard">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View records.location.report.wizard search -->
    <record id="view_records_location_report_wizard_search" model="ir.ui.view">
        <field name="name">view.records.location.report.wizard.search</field>
        <field name="model">records.location.report.wizard</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action records.location.report.wizard -->
    <record id="action_records_location_report_wizard" model="ir.actions.act_window">
        <field name="name">Records Location Report Wizard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">records.location.report.wizard</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Records Location Report Wizard.
            </p>
        </field>
    </record>

</odoo>
