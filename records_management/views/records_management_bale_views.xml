<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="records_management_bale_view_tree" model="ir.ui.view">
            <field name="name">paper.bale.view.tree</field>
            <field name="model">paper.bale</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name" />
                </list>
            </field>
        </record>
        <record id="records_management_bale_view_form" model="ir.ui.view">
            <field name="name">paper.bale.view.form</field>
            <field name="model">paper.bale</field>
            <field name="arch" type="xml">
                <form string="Records_management Bale">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" />
                            <h1>
                                <field name="name" placeholder="Name..." />
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="records_management_bale_view_search" model="ir.ui.view">
            <field name="name">paper.bale.view.search</field>
            <field name="model">paper.bale</field>
            <field name="arch" type="xml">
                <search>
                    <group expand="1" string="Group By">
                        <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}" />
                    </group>
                </search>
            </field>
        </record>
        <record id="action_records_management_bale" model="ir.actions.act_window">
            <!-- OPTIMIZATION: Model paper.bale has state field - consider using default search filters -->
            <field name="name">Records_management Bale</field>
            <field name="res_model">paper.bale</field>
            <field name="view_mode">list,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                There is no examples click here to add new Records_management Bale.
            </p>
            </field>
        </record>
    </data>
</odoo>
