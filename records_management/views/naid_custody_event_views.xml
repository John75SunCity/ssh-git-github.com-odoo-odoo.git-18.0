<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Tree view for naid.custody.event -->
    <record id="naid_custody_event_view_tree" model="ir.ui.view">
        <field name="name">naid.custody.event.view.tree</field>
        <field name="model">naid.custody.event</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for naid.custody.event -->
    <record id="naid_custody_event_view_form" model="ir.ui.view">
        <field name="name">naid.custody.event.view.form</field>
        <field name="model">naid.custody.event</field>
        <field name="arch" type="xml">
            <form string="Naid.Custody.Event">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for naid.custody.event -->
    <record id="naid_custody_event_view_search" model="ir.ui.view">
        <field name="name">naid.custody.event.view.search</field>
        <field name="model">naid.custody.event</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action for naid.custody.event -->
    <record id="action_naid_custody_event" model="ir.actions.act_window">
        <field name="name">Naid.Custody.Event</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">naid.custody.event</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to add a new naid.custody.event.
            </p>
        </field>
    </record>

</odoo>
