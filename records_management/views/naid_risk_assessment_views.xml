<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
<!-- Tree view for naid.risk.assessment -->
    <record id="naid_risk_assessment_view_tree" model="ir.ui.view">
        <field name="name">naid.risk.assessment.view.tree</field>
        <field name="model">naid.risk.assessment</field>
        <field name="type">list</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- Form view for naid.risk.assessment -->
    <record id="naid_risk_assessment_view_form" model="ir.ui.view">
        <field name="name">naid.risk.assessment.view.form</field>
        <field name="model">naid.risk.assessment</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Naid.Risk.Assessment">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for naid.risk.assessment -->
    <record id="naid_risk_assessment_view_search" model="ir.ui.view">
        <field name="name">naid.risk.assessment.view.search</field>
        <field name="model">naid.risk.assessment</field>
        <field name="type">search</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action for naid.risk.assessment -->
    <record id="action_naid_risk_assessment" model="ir.actions.act_window">
        <field name="name">Naid.Risk.Assessment</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">naid.risk.assessment</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to add a new naid.risk.assessment.
            </p>
        </field>
    </record>
</data>

</odoo>
