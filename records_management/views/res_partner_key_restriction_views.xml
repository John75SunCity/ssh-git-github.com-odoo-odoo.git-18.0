<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <data><!-- List view for base.rate -->
    <record id="res_partner_key_restriction_view_tree" model="ir.ui.view">
        <field name="name">res.partner.key.restriction.view.tree</field>
        <field name="model">res.partner.key.restriction</field>
        <field name="arch" type="xml">
        <list>
        <field name="name"/>
        </list>
    </field>
    </record>

<!-- Form view for base.rate -->
    <record id="res_partner_key_restriction_view_form" model="ir.ui.view">
        <field name="name">res.partner.key.restriction.view.form</field>
        <field name="model">res.partner.key.restriction</field>
        <field name="arch" type="xml">
        <form string="Res Partner Key Restriction">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
    </record>

<!-- Search view for base.rate -->
    <record id="res_partner_key_restriction_view_search" model="ir.ui.view">
        <field name="name">res.partner.key.restriction.view.search</field>
        <field name="model">res.partner.key.restriction</field>
        <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
    </record>

<!-- List view for Action -->
    <record id="action_res_partner_key_restriction" model="ir.actions.act_window">
        <field name="name">Res Partner Key Restriction</field>
        <field name="res_model">res.partner.key.restriction</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            There is no examples click here to add new Res Partner Key Restriction.
        </p>
    </field>
    </record>

    </data>
</odoo>
