<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View portal.feedback.communication View List -->
    <record id="view_portal_feedback_communication_list" model="ir.ui.view">
        <field name="name">view.portal.feedback.communication.list</field>
        <field name="model">portal.feedback.communication</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View portal.feedback.communication form -->
    <record id="view_portal_feedback_communication_form" model="ir.ui.view">
        <field name="name">view.portal.feedback.communication.form</field>
        <field name="model">portal.feedback.communication</field>
        <field name="arch" type="xml">
            <form string="Portal Feedback Communication">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View portal.feedback.communication search -->
    <record id="view_portal_feedback_communication_search" model="ir.ui.view">
        <field name="name">view.portal.feedback.communication.search</field>
        <field name="model">portal.feedback.communication</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action portal.feedback.communication -->
    <record id="action_portal_feedback_communication" model="ir.actions.act_window">
        <field name="name">Portal Feedback Communication</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">portal.feedback.communication</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Portal Feedback Communication.
            </p>
        </field>
    </record>

</odoo>
