<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View naid.audit.log View List -->
    <record id="view_naid_audit_log_list" model="ir.ui.view">
        <field name="name">view.naid.audit.log.list</field>
        <field name="model">naid.audit.log</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View naid.audit.log form -->
    <record id="view_naid_audit_log_form" model="ir.ui.view">
        <field name="name">view.naid.audit.log.form</field>
        <field name="model">naid.audit.log</field>
        <field name="arch" type="xml">
            <form string="Naid Audit Log">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View naid.audit.log search -->
    <record id="view_naid_audit_log_search" model="ir.ui.view">
        <field name="name">view.naid.audit.log.search</field>
        <field name="model">naid.audit.log</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action naid.audit.log -->
    <record id="action_naid_audit_log" model="ir.actions.act_window">
        <field name="name">Naid Audit Log</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">naid.audit.log</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Naid Audit Log.
            </p>
        </field>
    </record>

</odoo>
