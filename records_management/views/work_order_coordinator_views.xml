<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Tree view for work.order.coordinator -->
    <record id="view_work_order_coordinator_tree" model="ir.ui.view">
        <field name="name">work.order.coordinator.tree</field>
        <field name="model">work.order.coordinator</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for work.order.coordinator -->
    <record id="view_work_order_coordinator_form" model="ir.ui.view">
        <field name="name">work.order.coordinator.form</field>
        <field name="model">work.order.coordinator</field>
        <field name="arch" type="xml">
            <form string="Work Order Coordinator">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for work.order.coordinator -->
    <record id="view_work_order_coordinator_search" model="ir.ui.view">
        <field name="name">work.order.coordinator.search</field>
        <field name="model">work.order.coordinator</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action work.order.coordinator -->
    <record id="action_work_order_coordinator" model="ir.actions.act_window">
        <field name="name">Work Order Coordinator</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">work.order.coordinator</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Work Order Coordinator.
            </p>
        </field>
    </record>

</odoo>
