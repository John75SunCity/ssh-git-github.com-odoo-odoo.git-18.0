<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- List view for invoice.generation.log -->
    <record id="invoice_generation_log_view_tree" model="ir.ui.view">
        <field name="name">invoice.generation.log.view.tree</field>
        <field name="model">invoice.generation.log</field>
        <field name="arch" type="xml">
        <list>
        <field name="name"/>
        </list>
    </field>
    </record>

<!-- Form view for invoice.generation.log -->
    <record id="invoice_generation_log_view_form" model="ir.ui.view">
        <field name="name">invoice.generation.log.view.form</field>
        <field name="model">invoice.generation.log</field>
        <field name="arch" type="xml">
        <form string="Invoice.Generation.Log">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
    </record>

<!-- Search view for invoice.generation.log -->
    <record id="invoice_generation_log_view_search" model="ir.ui.view">
        <field name="name">invoice.generation.log.view.search</field>
        <field name="model">invoice.generation.log</field>
        <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
    </record>

<!-- Action for invoice.generation.log -->
    <record id="action_invoice_generation_log" model="ir.actions.act_window">
        <field name="name">Invoice.Generation.Log</field>
        <field name="res_model">invoice.generation.log</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            Click to add a new invoice.generation.log.
        </p>
    </field>
    </record>

</odoo>
