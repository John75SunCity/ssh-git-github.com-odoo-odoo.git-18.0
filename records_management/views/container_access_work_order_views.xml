<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View container.access.work.order View List -->
    <record id="view_container_access_work_order_list" model="ir.ui.view">
        <field name="name">view.container.access.work.order.list</field>
        <field name="model">container.access.work.order</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View container.access.work.order form -->
    <record id="view_container_access_work_order_form" model="ir.ui.view">
        <field name="name">view.container.access.work.order.form</field>
        <field name="model">container.access.work.order</field>
        <field name="arch" type="xml">
            <form string="Container Access Work Order">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View container.access.work.order search -->
    <record id="view_container_access_work_order_search" model="ir.ui.view">
        <field name="name">view.container.access.work.order.search</field>
        <field name="model">container.access.work.order</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action container.access.work.order -->
    <record id="action_container_access_work_order" model="ir.actions.act_window">
        <field name="name">Container Access Work Order</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">container.access.work.order</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Container Access Work Order.
            </p>
        </field>
    </record>

</odoo>
