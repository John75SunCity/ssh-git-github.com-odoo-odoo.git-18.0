<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View naid.compliance.alert View List -->
    <record id="view_naid_compliance_alert_list" model="ir.ui.view">
        <field name="name">view.naid.compliance.alert.list</field>
        <field name="model">naid.compliance.alert</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View naid.compliance.alert form -->
    <record id="view_naid_compliance_alert_form" model="ir.ui.view">
        <field name="name">view.naid.compliance.alert.form</field>
        <field name="model">naid.compliance.alert</field>
        <field name="arch" type="xml">
            <form string="Naid Compliance Alert">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View naid.compliance.alert search -->
    <record id="view_naid_compliance_alert_search" model="ir.ui.view">
        <field name="name">view.naid.compliance.alert.search</field>
        <field name="model">naid.compliance.alert</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action naid.compliance.alert -->
    <record id="action_naid_compliance_alert" model="ir.actions.act_window">
        <field name="name">Naid Compliance Alert</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">naid.compliance.alert</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Naid Compliance Alert.
            </p>
        </field>
    </record>

</odoo>
