<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Tree view for paper.bale.inspection.wizard -->
    <record id="view_paper_bale_inspection_wizard_tree" model="ir.ui.view">
        <field name="name">paper.bale.inspection.wizard.tree</field>
        <field name="model">paper.bale.inspection.wizard</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for paper.bale.inspection.wizard -->
    <record id="view_paper_bale_inspection_wizard_form" model="ir.ui.view">
        <field name="name">paper.bale.inspection.wizard.form</field>
        <field name="model">paper.bale.inspection.wizard</field>
        <field name="arch" type="xml">
            <form string="Paper Bale Inspection Wizard">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for paper.bale.inspection.wizard -->
    <record id="view_paper_bale_inspection_wizard_search" model="ir.ui.view">
        <field name="name">paper.bale.inspection.wizard.search</field>
        <field name="model">paper.bale.inspection.wizard</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action paper.bale.inspection.wizard -->
    <record id="action_paper_bale_inspection_wizard" model="ir.actions.act_window">
        <field name="name">Paper Bale Inspection Wizard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">paper.bale.inspection.wizard</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Paper Bale Inspection Wizard.
            </p>
        </field>
    </record>

</odoo>
