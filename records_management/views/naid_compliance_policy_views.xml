<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_naid_compliance_policy_list" model="ir.ui.view">
            <field name="name">view.naid.compliance.policy.list</field>
            <field name="model">naid.compliance.policy</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>
        <record id="view_naid_compliance_policy_form" model="ir.ui.view">
            <field name="name">view.naid.compliance.policy.form</field>
            <field name="model">naid.compliance.policy</field>
            <field name="arch" type="xml">
                <form string="Naid Compliance Policy">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" />
                            <h1>
                                <field name="name" placeholder="Name..." />
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_naid_compliance_policy_search" model="ir.ui.view">
            <field name="name">view.naid.compliance.policy.search</field>
            <field name="model">naid.compliance.policy</field>
            <field name="arch" type="xml">
                <search>
                    <group expand="1" string="Group By">
                        <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}" />
                    </group>
                </search>
            </field>
        </record>
        <record id="action_naid_compliance_policy" model="ir.actions.act_window">
            <!-- OPTIMIZATION: Model naid.compliance.policy has state field - consider using default search filters -->
            <field name="name">Naid Compliance Policy</field>
            <field name="res_model">naid.compliance.policy</field>
            <field name="view_mode>tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            There is no examples click here to add new Naid Compliance Policy.
        </p>
            </field>
        </record>
    </data>
</odoo>
