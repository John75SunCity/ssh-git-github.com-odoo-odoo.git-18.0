<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Tree view for signed.document.audit -->
    <record id="view_signed_document_audit_tree" model="ir.ui.view">
        <field name="name">signed.document.audit.tree</field>
        <field name="model">signed.document.audit</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for signed.document.audit -->
    <record id="view_signed_document_audit_form" model="ir.ui.view">
        <field name="name">signed.document.audit.form</field>
        <field name="model">signed.document.audit</field>
        <field name="arch" type="xml">
            <form string="Signed Document Audit">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for signed.document.audit -->
    <record id="view_signed_document_audit_search" model="ir.ui.view">
        <field name="name">signed.document.audit.search</field>
        <field name="model">signed.document.audit</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action signed.document.audit -->
    <record id="action_signed_document_audit" model="ir.actions.act_window">
        <field name="name">Signed Document Audit</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">signed.document.audit</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Signed Document Audit.
            </p>
        </field>
    </record>

</odoo>
