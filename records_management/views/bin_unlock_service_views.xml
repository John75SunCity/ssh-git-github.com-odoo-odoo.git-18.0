<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Tree view for bin.unlock.service -->
    <record id="view_bin_unlock_service_tree" model="ir.ui.view">
        <field name="name">bin.unlock.service.tree</field>
        <field name="model">bin.unlock.service</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- Form view for bin.unlock.service -->
    <record id="view_bin_unlock_service_form" model="ir.ui.view">
        <field name="name">bin.unlock.service.form</field>
        <field name="model">bin.unlock.service</field>
        <field name="arch" type="xml">
            <form string="Bin Unlock Service">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view for bin.unlock.service -->
    <record id="view_bin_unlock_service_search" model="ir.ui.view">
        <field name="name">bin.unlock.service.search</field>
        <field name="model">bin.unlock.service</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action bin.unlock.service -->
    <record id="action_bin_unlock_service" model="ir.actions.act_window">
        <field name="name">Bin Unlock Service</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">bin.unlock.service</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Bin Unlock Service.
            </p>
        </field>
    </record>

</odoo>
