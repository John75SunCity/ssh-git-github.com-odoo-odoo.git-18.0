<record id="processing_log_resolution_wizard_view_form" model="ir.ui.view">
        <field name="name">processing.log.resolution.wizard.view.form</field>
        <field name="model">processing.log.resolution.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1><field name="name" /></h1>
                    </div>
                    <group>
        <field name="log_id" />
        <field name="resolution" />
        <field name="resolution_date" />
                    </group>
                    <footer>
                        <button string="Resolve" type="object" name="action_resolve" class="btn-primary" />
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>


