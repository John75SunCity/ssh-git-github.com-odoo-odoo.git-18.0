<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View container.retrieval.work.order View List -->
<record id="container_retrieval_work_order_view_tree" model="ir.ui.view">
    <field name="name">container.retrieval.work.order.view.tree</field>
        <field name="model">container.retrieval.work.order</field>
        <field name="arch" type="xml">
<tree>
                <field name="name"/>
</tree>
        </field>
    </record>

    <!-- View container.retrieval.work.order form -->
<record id="container_retrieval_work_order_view_form" model="ir.ui.view">
    <field name="name">container.retrieval.work.order.view.form</field>
        <field name="model">container.retrieval.work.order</field>
        <field name="arch" type="xml">
            <form string="Container Retrieval Work Order">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View container.retrieval.work.order search -->
<record id="container_retrieval_work_order_view_search" model="ir.ui.view">
    <field name="name">container.retrieval.work.order.view.search</field>
        <field name="model">container.retrieval.work.order</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action container.retrieval.work.order -->
    <record id="action_container_retrieval_work_order" model="ir.actions.act_window">
        <field name="name">Container Retrieval Work Order</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">container.retrieval.work.order</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Container Retrieval Work Order.
            </p>
        </field>
    </record>

</odoo>
