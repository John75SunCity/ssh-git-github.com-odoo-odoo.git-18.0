<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- View customer.negotiated.rate View Tree -->
<record id="customer_negotiated_rate_view_tree" model="ir.ui.view">
<field name="name">customer.negotiated.rate.view.tree</field>
    <field name="model">customer.negotiated.rate</field>
    <field name="arch" type="xml">
<list>
            <field name="name"/>
</list>
    </field>
</record>

<!-- View customer.negotiated.rate form -->
<record id="customer_negotiated_rate_view_form" model="ir.ui.view">
    <field name="name">customer.negotiated.rate.view.form</field>
    <field name="model">customer.negotiated.rate</field>
    <field name="arch" type="xml">
        <form string="Customer Negotiated Rate">
            <sheet>
                <div class="oe_title">
                    <label for="name"/>
                    <h1>
                        <field name="name" placeholder="Name..."/>
                    </h1>
                </div>
            </sheet>
        </form>
    </field>
</record>

<!-- View customer.negotiated.rate search -->
<record id="customer_negotiated_rate_view_search" model="ir.ui.view">
    <field name="name">customer.negotiated.rate.view.search</field>
    <field name="model">customer.negotiated.rate</field>
    <field name="arch" type="xml">
        <search>
            <group expand="1" string="Group By">
                <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
            </group>
        </search>
    </field>
</record>

<!-- Action customer.negotiated.rate -->
<record id="action_customer_negotiated_rate" model="ir.actions.act_window">
    <field name="name">Customer Negotiated Rate</field>
    <field name="res_model">customer.negotiated.rate</field>
    <field name="view_mode">list,form</field>
    <field name="domain">[]</field>
    <field name="context">{}</field>
    <field name="help" type="html">
        <p class="oe_view_nocontent_create">
            There is no examples click here to add new Customer Negotiated Rate.
        </p>
    </field>
</record>

</odoo>
