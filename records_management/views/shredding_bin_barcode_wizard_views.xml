<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
<!-- View shredding.bin.barcode.wizard View List -->
    <record id="shredding_bin_barcode_wizard_view_list" model="ir.ui.view">
        <field name="name">shredding.bin.barcode.wizard.view.list</field>
        <field name="model">shredding.bin.barcode.wizard</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <!-- View shredding.bin.barcode.wizard form -->
    <record id="shredding_bin_barcode_wizard_view_form" model="ir.ui.view">
        <field name="name">shredding.bin.barcode.wizard.view.form</field>
        <field name="model">shredding.bin.barcode.wizard</field>
        <field name="arch" type="xml">
            <form string="Shredding Bin Barcode Wizard">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View shredding.bin.barcode.wizard search -->
    <record id="shredding_bin_barcode_wizard_view_search" model="ir.ui.view">
        <field name="name">shredding.bin.barcode.wizard.view.search</field>
        <field name="model">shredding.bin.barcode.wizard</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action shredding.bin.barcode.wizard -->
    <record id="shredding_bin_barcode_wizard_action" model="ir.actions.act_window">
        <field name="name">Shredding Bin Barcode Wizard</field>
        <field name="res_model">shredding.bin.barcode.wizard</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new Shredding Bin Barcode Wizard.
            </p>
        </field>
    </record>
</data>

</odoo>
