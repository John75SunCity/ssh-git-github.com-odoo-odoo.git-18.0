<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Tree View -->
        <record id="full_customization_name_view_tree" model="ir.ui.view">
            <field name="name">full_customization_name.view.tree</field>
            <field name="model">full_customization_name</field>
            <field name="arch" type="xml">
                <tree string="Full Customization Name">
                    <field name="name"/>
                    <field name="description"/>
                    <field name="active"/>
                    <field name="company_id"/>
                    <field name="user_id"/>
                </tree>
            </field>
        </record>

        <!-- Form View -->
        <record id="full_customization_name_view_form" model="ir.ui.view">
            <field name="name">full_customization_name.view.form</field>
            <field name="model">full_customization_name</field>
            <field name="arch" type="xml">
                <form string="Full Customization Name">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                            <field name="active"/>
                            <field name="company_id"/>
                            <field name="user_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Search View -->
        <record id="full_customization_name_view_search" model="ir.ui.view">
            <field name="name">full_customization_name.view.search</field>
            <field name="model">full_customization_name</field>
            <field name="arch" type="xml">
                <search string="Search Full Customization Name">
                    <field name="name"/>
                    <field name="description"/>
                </search>
            </field>
        </record>

        <!-- Action -->
        <record id="action_full_customization_name" model="ir.actions.act_window">
            <field name="name">Full Customization Name</field>
            <field name="res_model">full_customization_name</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- Menu -->
        <!--
        <menuitem
            id="menu_full_customization_name"
            name="Full Customization Name"
            action="action_full_customization_name"
            parent="REPLACE_WITH_PARENT_MENU_ID"
            sequence="10"/>
        -->

    </data>
</odoo>
