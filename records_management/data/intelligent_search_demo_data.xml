        <!-- Intelligent Search Configuration Settings -->
        <record id="config_search_parameters" model="ir.config_parameter">
            <field name="key">intelligent_search.config</field>
            <field name="value">{
                "fuzzy_matching_threshold": 0.8,
                "enable_phonetic_search": true,
                "max_search_results": 50,
                "search_timeout_seconds": 30,
                "enable_partial_matches": true,
                "boost_recent_documents": true,
                "boost_factor_recent": 1.5,
                "enable_synonym_expansion": true,
                "min_search_length": 3
            }</field>
        </record>

        <record id="config_search_synonyms" model="ir.config_parameter">
            <field name="key">intelligent_search.synonyms</field>
            <field name="value">{
                "medical": ["health", "healthcare", "clinical", "patient", "medical records"],
                "financial": ["accounting", "finance", "money", "billing", "invoices"],
                "legal": ["law", "contract", "agreement", "litigation", "court"],
                "personnel": ["HR", "human resources", "employee", "staff", "payroll"],
                "insurance": ["coverage", "claims", "policy", "premium", "deductible"],
                "project": ["initiative", "program", "development", "implementation"]
            }</field>
        </record>

        <record id="config_search_security" model="ir.config_parameter">
            <field name="key">intelligent_search.security</field>
            <field name="value">{
