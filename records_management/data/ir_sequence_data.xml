        <record id="seq_bin_64_gallon" model="ir.sequence">
            <field name="name">64 Gallon Bin Barcode Sequence</field>
            <field name="code">shredding.bin.64gallon</field>
            <field name="prefix">BIN64</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for 96 Gallon Bins -->
        <record id="seq_bin_96_gallon" model="ir.sequence">
            <field name="name">96 Gallon Bin Barcode Sequence</field>
            <field name="code">shredding.bin.96gallon</field>
            <field name="prefix">BIN96</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
