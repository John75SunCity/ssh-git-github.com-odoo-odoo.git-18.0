<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <data>
        <!-- Sequence for Records Container -->
        <record id="seq_records_container" model="ir.sequence">
            <field name="name">Records Container Sequence</field>
            <field name="code">records.container</field>
            <field name="prefix">CONT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Records Document -->
        <record id="seq_records_document" model="ir.sequence">
            <field name="name">Records Document Sequence</field>
            <field name="code">records.document</field>
            <field name="prefix">DOC</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Pickup Requests -->
        <record id="seq_pickup_request" model="ir.sequence">
            <field name="name">Pickup Request Sequence</field>
            <field name="code">pickup.request</field>
            <field name="prefix">PICKUP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Shredding Service -->
        <record id="seq_shredding_service" model="ir.sequence">
            <field name="name">Shredding Service Sequence</field>
            <field name="code">shredding.service</field>
            <field name="prefix">SHRED</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Deletion Requests -->
        <record id="seq_records_deletion_request" model="ir.sequence">
            <field name="name">Records Deletion Request Sequence</field>
            <field name="code">records.deletion.request</field>
            <field name="prefix">DEL</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Container Numbers -->
        <record id="seq_records_container_barcode" model="ir.sequence">
            <field name="name">Records Container Number Sequence</field>
            <field name="code">records.container.number</field>
            <field name="prefix">CN</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Shredding Work Orders -->
        <record id="seq_work_order_shredding" model="ir.sequence">
            <field name="name">Shredding Work Order Sequence</field>
            <field name="code">work.order.shredding</field>
            <field name="prefix">WRK</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Shredding Bin Barcodes -->
        <record id="seq_shredding_bin_barcode" model="ir.sequence">
            <field name="name">Shredding Bin Barcode Sequence (Legacy)</field>
            <field name="code">shredding.bin.barcode</field>
            <field name="prefix"></field>
            <field name="padding">10</field>
            <field name="company_id" eval="False"/>
            <field name="active">False</field>
        </record>

        <!-- Sequence for 23 Gallon Bins -->
        <record id="seq_bin_23_gallon" model="ir.sequence">
            <field name="name">23 Gallon Bin Barcode Sequence</field>
            <field name="code">shredding.bin.23gallon</field>
            <field name="prefix">BIN23</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for 32 Gallon Console -->
        <record id="seq_console_32_gallon" model="ir.sequence">
            <field name="name">32 Gallon Console Barcode Sequence</field>
            <field name="code">shredding.console.32gallon</field>
            <field name="prefix">CON32</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for 32 Gallon Bins -->
        <record id="seq_bin_32_gallon" model="ir.sequence">
            <field name="name">32 Gallon Bin Barcode Sequence</field>
            <field name="code">shredding.bin.32gallon</field>
            <field name="prefix">BIN32</field>
            <field name="padding">5</field>
