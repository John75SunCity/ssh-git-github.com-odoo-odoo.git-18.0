            <field name="code">document.retrieval.work.order</field>
            <field name="prefix">RET</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for NAID Certificates -->
        <record id="seq_naid_certificate" model="ir.sequence">
            <field name="name">NAID Certificate Sequence</field>
            <field name="code">naid.certificate</field>
            <field name="prefix">NAID</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
