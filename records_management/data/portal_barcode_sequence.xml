<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Portal barcode sequence for generating temporary barcodes -->
    <record id="seq_portal_barcode" model="ir.sequence">
        <field name="name">Portal Barcode</field>
        <field name="code">portal.barcode</field>
        <field name="prefix">PB</field>
        <field name="padding">7</field>
        <field name="implementation">no_gap</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    </data>
</odoo>
