<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Demo partner data -->
    <record id="demo_customer_1" model="res.partner">
        <field name="name">Demo Records Management Customer</field>
        <field name="email">demo.customer@example.com</field>
        <field name="phone">(123) 456-7890</field>
        <field name="street">123 Storage Lane</field>
        <field name="city">Records City</field>
        <field name="zip">12345</field>
        <field name="country_id" ref="base.us"/>
        <field name="is_company">True</field>
        <field name="customer_rank">1</field>
    </record>

    <!-- Demo department -->
    <record id="demo_department_1" model="records.department">
        <field name="name">Demo Department</field>
        <field name="code">DEMO</field>
        <field name="partner_id" ref="demo_customer_1"/>
    </record>

    <!-- Demo location -->
    <record id="demo_location_1" model="records.location">
        <field name="name">Demo Warehouse A</field>
        <field name="code">DEMO-A</field>
        <field name="location_type">warehouse</field>
        <field name="storage_capacity">100</field>
    </record>

    <!-- Demo container -->
    <record id="demo_container_1" model="records.container">
        <field name="name">DEMO-CONT-001</field>
        <field name="partner_id" ref="demo_customer_1"/>
        <field name="department_id" ref="demo_department_1"/>
        <field name="location_id" ref="demo_location_1"/>
        <field name="state">active</field>
    </record>
</odoo>
